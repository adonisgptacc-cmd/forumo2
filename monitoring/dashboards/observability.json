{
  "title": "Forumo - API Overview",
  "panels": [
    { "type": "graph", "title": "HTTP Requests", "targets": [{ "expr": "sum(rate(http_requests_total[5m]))" }] },
    { "type": "graph", "title": "Request Duration p95", "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket[5m])) by (le))" }] },
    { "type": "graph", "title": "DB Query Duration", "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_ms_bucket[5m])) by (le))" }] },
    { "type": "stat", "title": "Cache Hit Ratio", "targets": [{ "expr": "avg(cache_hit_ratio)" }] }
  ]
}
